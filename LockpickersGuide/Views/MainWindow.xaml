<c:AdvancedWindow x:Class="LockpickersGuide.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:c="clr-namespace:LockpickersGuide.ViewLogic"
        xmlns:local="clr-namespace:LockpickersGuide" d:DataContext="{d:DesignInstance Type=vm:MainWindowViewModel}"
        xmlns:vm="clr-namespace:LockpickersGuide.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="600" Height="600" MinWidth="600" Width="1200" WindowStartupLocation="CenterScreen" MouseDown="Window_MouseDown" Style="{StaticResource MainWindowStyle}">
    <Grid>
        <Grid>
            <Frame x:Name="FRM_Main" Source="Preload.xaml" Width="auto" Height="auto" Margin="10,10,10,10" NavigationUIVisibility="Hidden" />
        </Grid>
        <Border CornerRadius="20" BorderThickness="0" Opacity="0.6" Background="#000011" Visibility="{Binding GreyOutVisibility}" d:Visibility="Hidden" />
    </Grid>
</c:AdvancedWindow>
